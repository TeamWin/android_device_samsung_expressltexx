    chown system system /sys/class/misc/backlightnotification/enabled
    chown system system /sys/class/misc/backlightnotification/blink_control
    chown system system /sys/class/misc/backlightnotification/notification_led
    restorecon /sys/class/misc/backlightnotification/enabled

    # PCC
    chown system system /sys/devices/virtual/graphics/fb0/rgb
    chmod 0660 /sys/devices/virtual/graphics/fb0/rgb

# Start camera server as daemon
service qcamerasvr /system/bin/mm-qcamera-daemon
    class late_start
    user camera
    group camera system inet input graphics

service thermald /system/bin/thermald
    class main
    user root
    group root

service mpdecision /system/bin/mpdecision --avg_comp
    class main
    user root
    group root system readproc
    disabled

service qseecomd /system/bin/qseecomd
    class core
    user system
    group system

service charger /sbin/healthd -c
    class charger
    critical
    seclabel u:r:healthd:s0

service geomagneticd /system/bin/geomagneticd
    class late_start
    user compass
    group system input

service time_daemon /system/bin/time_daemon
    class late_start
    user root
    group root

service wcnss-service /system/bin/wcnss_service
    class main
    user system
    group system wifi radio
    oneshot

# insthk
service insthk /system/bin/insthk
    class main
    user root
    disabled
    oneshot
    seclabel u:r:insthk:s0

on property:sys.qseecomd.enable=true
    start insthk
